{
  "schemaVersion": 1,
  "id": "numbyhack",
  "version": "${version}",
  "name": "Numby Hack",
  "description": "一个简单的 Meteor 插件，旨在为用户提供更多自定义选项。由 Number81 制作。汉化：zip8919，请配合彗星中文版使用！",
  "authors": [
    "cqb13"
  ],
  "contact": {
    "repo": "https://github.com/cqb13/Numby-hack",
    "sources": "https://github.com/cqb13/Numby-hack",
    "issues": "https://github.com/cqb13/Numby-hack/issues"
  },
  "icon": "assets/numby-hack/textures/icon.png",
  "environment": "client",
  "entrypoints": {
    "meteor": [
      "cqb13.NumbyHack.NumbyHack"
    ]
  },
  "mixins": [
    "NumbyHack.mixins.json"
  ],
  "accessWidener": "accesswidener",
  "custom": {
    "meteor-client:color": "146,188,98"
  },
  "depends": {
    "java": ">=17",
    "minecraft": ">=${mc_version}",
    "meteor-client": ">=0.5.5"
  }
}
